{% extends 'base.html.twig' %}

{% block title %}Family{% endblock %}

{% block body %}
    <h1>Family</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ family.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ family.name }}</td>
            </tr>
            <tr>
                <th>HeadOfHousehold</th>
                <td>{{ family.headOfHousehold }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ family.address }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ family.city }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ family.country }}</td>
            </tr>
            <tr>
                <th>Contact</th>
                <td>{{ family.contact }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_family_index') }}">back to list</a>

    <a href="{{ path('app_family_edit', {'id': family.id}) }}">edit</a>

    {{ include('family/_delete_form.html.twig') }}
{% endblock %}
